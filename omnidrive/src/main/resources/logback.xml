<?xml version="1.0" encoding="UTF-8"?>
<configuration>

    <appender name="STDOUT" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%magenta([%mdc{logCorrelationId:-undefined}]) %date{ISO8601} %highlight(%-5level) %logger{5} - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="INFO">
        <appender-ref ref="STDOUT" />
    </root>

    <logger name="com.pro.jenova.omnidrive.rest.filter.RequestAndResponseFilter" level="DEBUG"/>

</configuration>